{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/read-n-write/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/read-n-write/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/read-n-write",
  "projects": {
    "read-n-write": {
      "type": "application",
      "root": "apps/read-n-write",
      "entryFile": "main",
      "sourceRoot": "apps/read-n-write/src",
      "compilerOptions": {
        "tsConfigPath": "apps/read-n-write/tsconfig.app.json"
      }
    },
    "reader": {
      "type": "application",
      "root": "apps/reader",
      "entryFile": "main",
      "sourceRoot": "apps/reader/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reader/tsconfig.app.json"
      }
    }
  }
}